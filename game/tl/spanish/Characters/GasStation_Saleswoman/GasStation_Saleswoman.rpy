    "Por..."
    "Por el hecho de que ..."

# game/Characters/GasStation_Saleswoman/GasStation_Saleswoman.rpy:765
translate spanish gas_saleswoman_scene3_28570c9e:

    # m "Ну??"
    m "¿¿Bien??"

