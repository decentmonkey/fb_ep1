    # m "Ну, я жду?"
    m "¿Entonces estoy esperando?"

# game/Characters/GasStation_Saleswoman/GasStation_Saleswoman.rpy:758
translate spanish gas_saleswoman_scene3_edec20ee:

    # saleswoman "Я прошу прощения..."
    # "За..."
    # "За то, что...."
